page {
  background: #fbbd08;
}
.title{
  color: white;
  font-size: 40rpx;
  font-weight: 600;
}

.userAvatar {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  color: #fff;
  margin-top: 10rpx;
}

.userAvatarurl {
  overflow: hidden;
  display: block;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  border: 2px solid #fff;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}


.tips{
  margin-top: 20rpx;
  margin-bottom: 30rpx;
}
.tips text{
  font-size: 45rpx;
  font-weight: 600;
   color: #e54d42;
  
}

.list{
  margin-bottom: 200rpx;
}

.cu-chat .cu-item {
	display: flex;
	padding: 30rpx 30rpx 0rpx;
	position: relative;
  
}

.sumittask{
  z-index: 888;
  width: 100%;
  position: fixed;
  bottom: 0;
  height: 130rpx;
  margin-top: 80rpx;
   background-color: #fbbd08;
}
.sumittask button{
  width: 280rpx;
}